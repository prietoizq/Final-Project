<%= form_for [@user, @image], :html => { :multipart => true} do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<br>

	<%= f.label :direction %>
	<%= f.text_field :direction %>
	<br>

	<%= f.label :description %>
	<%= f.text_field :description %>
	<br>

	<%= f.label :rating %>
	<%= f.number_field :rating %>
	<br>

	<%= f.label :photo %>
	<%= f.file_field :photo %>
	<br>

	<%= f.submit 'Save' %>

<% end %>